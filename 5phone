#include <iostream>
using namespace std;
struct phone {
    int area_code;
    int exchange;
    int number;
};
int main() {
    phone p1, p2;
    p1 = {212, 767, 8900};  // Initialize one phone number

    // Ask the user for their phone number
    cout << "Enter your area code, exchange, and number: ";
    cin >> p2.area_code >> p2.exchange >> p2.number;

    // Display both numbers
    cout << "My number is (" << p1.area_code << ") " 
         << p1.exchange << "-" << p1.number << endl;

    cout << "Your number is (" << p2.area_code << ") " 
         << p2.exchange << "-" << p2.number << endl;
    return 0;
}
